
QTCOREINCL = @QTCOREINCL@
QTGUIINCL = @QTGUIINCL@
QTCORELIBS = @QTCORELIBS@
QTGUILIBS = @QTGUILIBS@

CXXFLAGS = -Wall -O2 $(QTCOREINCL) $(QTGUIINCL) -I../libntreg

SOURCE = \
	regedit.cpp \
	registryitem.cpp

OBJECTS = $(SOURCE:%.cpp=%.o)

TARGET = regedit

NTREGLIB = ../libntreg/libntreg.a

$(TARGET): $(OBJECTS) $(NTREGLIB)
	$(CXX) -o $@ $^ $(QTCORELIBS) $(QTGUILIBS)

clean:
	rm -f $(TARGET) $(OBJECTS)
